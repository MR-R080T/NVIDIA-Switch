sudo modprobe bbswitch
sudo rmmod nvidia
sudo tee /proc/acpi/bbswitch <<< OFF
sudo rm /etc/X11/xorg.conf
sudo cp $INS_DIR/xorg.conf-intel /etc/X11/xorg.conf
rm ~/.xinitrc
cp $INS_DIR/xinitrc-intel ~/.xinitrc
sudo eselect opengl set xorg-x11
#ADD WINDOW MANAGER CONFIG CHANGES HERE
#rm ~/.i3/config
#cp $INS_DIR/config-i3-intel ~/.i3/config
#STARTING X
startx